<Window x:Class="ARM_Vyz.Views.DessertationAdd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ARM_Vyz.Views"
        mc:Ignorable="d" Closed="Window_Closed"
        Title="DessertationAdd" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Label Content="NameDissertation"/>
            <TextBox x:Name="tbName" Text="Des1"/>
            <Label Content="Year"/>
            <DatePicker x:Name="dpYear" SelectedDate="12.3.2002"/>
            <Button x:Name="btnAdd" Content="Add" Click="btnAdd_Click"/>
        </StackPanel>

    </Grid>
</Window>
